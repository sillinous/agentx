# Unified Media Asset Manager - Environment Configuration
# Copy this file to .env and fill in your actual values

# Anthropic API Key for AI agents (required for production, optional for testing with mock providers)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration (defaults shown)
DATABASE_URL=sqlite:///./sql_app.db

# Authentication (set to true to disable auth for development)
DISABLE_AUTH=true

# CORS Origins
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Celery Configuration
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Optional: Video/Audio API Keys (for real providers, not needed for mock testing)
# RUNWAY_API_KEY=your_runway_key
# MINIMAX_API_KEY=your_minimax_key
# KLINGAI_API_KEY=your_klingai_key
# SUNO_API_KEY=your_suno_key
# ELEVENLABS_API_KEY=your_elevenlabs_key
