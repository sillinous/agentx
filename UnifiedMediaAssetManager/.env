# Unified Media Asset Manager - Environment Configuration
# Development settings with mock providers

# Anthropic API Key (using mock for testing - replace with real key for production)
ANTHROPIC_API_KEY=mock_key_for_testing

# Database Configuration
DATABASE_URL=sqlite:///./sql_app.db

# Authentication
DISABLE_AUTH=true

# CORS Origins
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Celery Configuration
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0
